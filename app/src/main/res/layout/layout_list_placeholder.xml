<?xml version="1.0" encoding="utf-8"?>
<com.facebook.shimmer.ShimmerFrameLayout xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:background="@color/white"
	android:id="@+id/layout_list_placeholder"
	android:layout_width="match_parent"
	android:layout_height="0dp"
	android:layout_gravity="center"
	android:orientation="vertical"
	android:touchscreenBlocksFocus="true"
	app:layout_constraintTop_toBottomOf="@id/toolbar"
	app:layout_constraintBottom_toBottomOf="parent"
	app:layout_constraintStart_toStartOf="parent"
	app:layout_constraintEnd_toEndOf="parent"
	tools:showIn="@layout/activity_user_list">

	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />

			<include layout="@layout/item_placeholder_user" />
		</LinearLayout>
	</ScrollView>
</com.facebook.shimmer.ShimmerFrameLayout>
